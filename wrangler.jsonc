{
  "$schema": "node_modules/wrangler/config-schema.json",
  
  "assets": {
    "directory": "./dist",
    "not_found_handling": "single-page-application"
  },
  
  "routes": [
    {
      "pattern": "sitemap.xml",
      "custom_domain": true
    },
    {
      "pattern": "sitemap_index.xml",
      "custom_domain": true
    },
    {
      "pattern": "*",
      "custom_domain": true
    }
  ]
}
