{
  "$schema": "node_modules/wrangler/config-schema.json",

  "name": "thefuturemed",
  "compatibility_date": "2025-09-18",
  
  "assets": {
    "directory": "./dist",
    "not_found_handling": "single-page-application"
  },
  
  "routes": [
    {
      "pattern": "sitemap.xml",
      "custom_domain": true
    },
    {
      "pattern": "sitemap_index.xml",
      "custom_domain": true
    },
    {
      "pattern": "www.thefuturemed.com/*",
      "custom_domain": true
    }
  ]
}